{
  "name": "@heathmont/moon-charts",
  "sideEffects": false,
  "version": "8.3.5",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "typings": "lib/index.d.ts",
  "repository": "https://github.com/coingaming/moon-design",
  "author": "Yolo Group",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run build:index && pnpm run build:code",
    "build:index": "barrelsby --config ../../.config/barrelsby/barrelsby.json --directory src",
    "build:code": "pnpm run build:js:es && pnpm run build:js:commonjs",
    "build:js:commonjs": "tsc --project tsconfig.cjs.json",
    "build:js:es": "tsc --project tsconfig.esm.json",
    "chokidar": "chokidar \"**/*.tsx\" \"**/*.ts\" -i \"lib/**/*.d.ts\" -c \"pnpm run build:code\" "
  },
  "dependencies": {
    "@heathmont/moon-assets": "workspace:^8.3.5",
    "@heathmont/moon-components": "workspace:^8.3.5",
    "@heathmont/moon-themes": "workspace:^8.3.5",
    "@heathmont/moon-utils": "workspace:^8.3.5",
    "@types/react-simple-maps": "1.0.3",
    "@types/react-transition-group": "4.4.0",
    "react-simple-maps": "2.3.0",
    "react-transition-group": "4.4.1",
    "react-use": "15.3.8",
    "recharts": "2.0.9"
  },
  "peerDependencies": {
    "react": ">= 17.0.2",
    "react-dom": ">= 17.0.2",
    "styled-components": ">= 5.3.3"
  }
}
