{
  "private": true,
  "name": "@heathmont/sportsbet-user-portal",
  "version": "0.78.0",
  "main": "dist/main.bundle.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coingaming/sportsbet-design.git"
  },
  "author": "mBet Solutions N.V.",
  "license": "MIT",
  "scripts": {
    "build": "export NODE_ENV=production && yarn build:assets && yarn build:client && yarn build:server",
    "build:server": "webpack --config .config/webpack/webpack.config.server.js --display-error-details",
    "build:client": "webpack --config .config/webpack/webpack.config.client.js --display-error-details",
    "build:assets": "mkdir -p dist/assets && cp -R assets/ dist/assets/",
    "bundlesize": "bundlesize",
    "clean": "rimraf **/{lib,dist,.cache,public}/",
    "dev": "export NODE_ENV=development && webpack --config .config/webpack/webpack.config.isomorphic.js --watch --progress & nodemon ./dist/server.bundle.js",
    "production": "export NODE_ENV=production && yarn build && yarn serve",
    "serve": "wait-on ./dist/server.bundle.js && node ./dist/server.bundle.js"
  },
  "bundlesize": [
    {
      "path": "./dist/main.bundle.js",
      "maxSize": "175 kB",
      "compression": "gzip"
    }
  ],
  "dependencies": {
    "@emotion/cache": "10.0.9",
    "@emotion/core": "10.0.10",
    "@emotion/styled": "10.0.9",
    "@heathmont/sportsbet-components": "^0.78.0",
    "@heathmont/sportsbet-global": "^0.78.0",
    "@heathmont/sportsbet-icons": "^0.78.0",
    "@heathmont/sportsbet-portals-shared": "^0.78.0",
    "@heathmont/sportsbet-tokens": "^0.78.0",
    "@heathmont/sportsbet-user-portal-components": "^0.78.0",
    "@heathmont/sportsbet-utils": "^0.78.0",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "create-emotion-server": "10.0.0",
    "dotenv-safe": "6.1.0",
    "express": "4.16.3",
    "polished": "3.0.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-router": "4.3.1",
    "react-router-dom": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-transform-typescript": "7.3.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@types/dotenv-safe": "5.0.3",
    "@types/express": "4.16.1",
    "@types/react-router": "4.4.5",
    "@types/react-router-dom": "4.3.2",
    "bundlesize": "0.17.1",
    "nodemon": "1.18.10",
    "wait-on": "3.2.0",
    "webpack": "4.29.3",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "description": "> **Note**: this package is temporary to ease user portal development alongside the Design System. It will be moved from the repo upon completion.",
  "bugs": {
    "url": "https://github.com/coingaming/sportsbet-design/issues"
  },
  "homepage": "https://github.com/coingaming/sportsbet-design#readme"
}
